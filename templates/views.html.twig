<h4 class="text-danger type" data-toggle="collapse"
    data-target="#views">Views</h4>
<div class="collapse {{ collapse }}" id="views">

    <table class="table table-striped">
        <thead class="thead-dark">
        <tr>
            <th>Taxonomy Name</th>
            <th>Machine name</th>
        </tr>
        </thead>
        <tbody>
        {% for v in views %}
            <tr>
                <td>{{ v.label }}</td>
                <td>{{ v.id }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
